<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<templates>
		<template 
			autoinsert="true" 
			context="org.eclipse.ocl.examples.xtext.completeocl.CompleteOCL.ContextDeclCS" 
			deleted="false" 
			description="context declaration" 
			enabled="true" 
			name="Context"
			id="ContextDeclCS">context ${cursor}${type}
inv ${cursor}${name} :
	${cursor}${expression}
</template>
		<template 
			autoinsert="true" 
			context="org.eclipse.ocl.examples.xtext.completeocl.CompleteOCL.DefCS" 
			deleted="false" 
			description="def declaration" 
			enabled="true" 
			name="Def"
			id="DefCS">def : ${cursor}${name} : ${value:CrossReference('TypeExpCS')} = ${expression}</template>
		<template 
			autoinsert="true" 
			context="org.eclipse.ocl.examples.xtext.completeocl.CompleteOCL.ImportCS" 
			deleted="false" 
			description="import statement" 
			enabled="true" 
			name="Import"
			id="ImportCS">import ${cursor}${importURI}</template>
		<template 
			autoinsert="true" 
			context="org.eclipse.ocl.examples.xtext.completeocl.CompleteOCL.InvCS" 
			deleted="false" 
			description="invariant declaration" 
			enabled="true" 
			name="Invariant"
			id="InvCS">inv ${cursor}${name} :
	${cursor}${expression}</template>
	<template 
		autoinsert="true" 
		context="org.eclipse.ocl.examples.xtext.essentialocl.EssentialOCL.LetExpCS" 
		deleted="false" 
		description="let statement" 
		enabled="true" 
		id="LetExpCS" 
		name="Let">let ${cursor}${variableName} = ${cursor}${init} in
	${line_selection}${expression}
		</template>
	<template 
		autoinsert="true" 
		context="org.eclipse.ocl.examples.xtext.completeocl.CompleteOCL.OperationContextDeclCS" 
		deleted="false" 
		description="operation context declaration" 
		enabled="true" 
		name="Operation Context"
		id="OperationContextDeclCS">context ${cursor}${value:CrossReference('Class')}}::${cursor}${operation}() : ${cursor}${returnType}</template>
	<template 
		autoinsert="true" 
		context="org.eclipse.ocl.examples.xtext.completeocl.CompleteOCL.OperationContextDeclCS" 
		deleted="false" 
		description="operation context declaration with body" 
		enabled="true" 
		name="Operation Context"
		id="OperationContextDeclCS2">context ${cursor}${value:CrossReference('Class')}}::${cursor}${operation}() : ${cursor}${returnType}
body: ${cursor}${expression}</template>
	<template 
		autoinsert="true" 
		context="org.eclipse.ocl.examples.xtext.completeocl.CompleteOCL.OperationContextDeclCS" 
		deleted="false" 
		description="operation context declaration with conditions" 
		enabled="true" 
		name="Operation Context"
		id="OperationContextDeclCS3">context ${cursor}${value:CrossReference('Class')}}::${cursor}${operation}() : ${cursor}${returnType}
pre: ${cursor}${pre_expression}
post: ${cursor}${post_expression}</template>
	<template 
		autoinsert="true" 
		context="org.eclipse.ocl.examples.xtext.completeocl.CompleteOCL.PackageDeclarationCS" 
		deleted="false" 
		description="package declaration" 
		enabled="true" 
		name="Package"
		id="PackageDeclarationCS">package ${cursor}${name}
	${cursor}${line_selection}
endpackage</template>
	<template 
		autoinsert="true" 
		context="org.eclipse.ocl.examples.xtext.completeocl.CompleteOCL.PostCS" 
		deleted="false" 
		description="post condition" 
		enabled="true" 
		name="Post Condition"
		id="PostCS">post: ${cursor}${variable} = ${cursor}${expression}</template>
	<template 
		autoinsert="true" 
		context="org.eclipse.ocl.examples.xtext.essentialocl.EssentialOCL.BinaryOperatorCS" 
		deleted="false" 
		description="pre condition" 
		enabled="true" 
		name="Pre Condition"
		id="PreCS">pre: ${cursor}${expression}</template>
	<template 
		autoinsert="true" 
		context="org.eclipse.ocl.examples.xtext.completeocl.CompleteOCL.PropertyContextDeclCS" 
		deleted="false" 
		description="property context declaration" 
		enabled="true" 
		name="Property Context"
		id="PropertyContextDeclCS">context ${cursor}${type}::${cursor}${property} : ${cursor}${propertyType}</template>
	<template 
		autoinsert="true" 
		context="org.eclipse.ocl.examples.xtext.completeocl.CompleteOCL.PropertyContextDeclCS" 
		deleted="false" 
		description="init property context declaration" 
		enabled="true" 
		name="Property Context"
		id="PropertyContextDeclCS2">context ${cursor}${type}::${cursor}${property} : ${cursor}${propertyType}
	init: 
		${cursor}${expression}</template>
	<template 
		autoinsert="true" 
		context="org.eclipse.ocl.examples.xtext.completeocl.CompleteOCL.PropertyContextDeclCS" 
		deleted="false" 
		description="derive property context declaration" 
		enabled="true" 
		name="Property Context"
		id="PropertyContextDeclCS3">context ${cursor}${type}::${cursor}${property} : ${cursor}${propertyType}
	derive: 
		${cursor}${expression}</template>
	<template 
		autoinsert="true" 
		context="org.eclipse.ocl.examples.xtext.essentialocl.EssentialOCL.TupleLiteralExpCS" 
		deleted="false" 
		description="Tuple Literal" 
		enabled="true" 
		name="Tuple Literal"
		id="TupleLiteralExpCS">Tuple { ${cursor}${name} : ${type} = ${expression} }</template>
	<template 
		autoinsert="true" 
		context="org.eclipse.ocl.examples.xtext.completeocl.CompleteOCL.LibraryCS" 
		deleted="false" 
		description="library declaration" 
		enabled="true" 
		name="Library"
		id="LibraryCS">library ${cursor}${packageURI}</template>
	<template 
		autoinsert="true" 
		context="org.eclipse.ocl.examples.xtext.completeocl.CompleteOCL.IncludeCS" 
		deleted="false" 
		description="include declaration" 
		enabled="true" 
		name="Include"
		id="IncludeCS">include ${cursor}${namespace}</template>
	<template 
		autoinsert="true" 
		context="org.eclipse.ocl.examples.xtext.essentialocl.EssentialOCL.TupleTypeCS" 
		deleted="false" 
		description="Tuple Type" 
		enabled="true" 
		name="TupleType"
		id="TupleTypeCS">Tuple ( ${cursor} ${name} : ${value:CrossReference('TypeExpCS')} )</template>
	<template 
			autoinsert="true" 
			context="org.eclipse.ocl.examples.xtext.essentialocl.EssentialOCL.IfExpCS" 
			deleted="false" 
			description="if then else statement" 
			enabled="true" 
			name="if then else"
			id="IfExpCS">if ${cursor}${condition} then
	${line_selection}${then_expression}
else
		${line_selection}${else_expression}
endif
</template>
</templates>
